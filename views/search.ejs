<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= typeof title !== 'undefined' ? title : 'Kinetic Constructs' %></title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <%- include('partials/header') %>

    <main>
        <%- include('partials/search-main', {
            products: typeof products !== 'undefined' ? products : [],
            error: typeof error !== 'undefined' ? error : null,
            hierarchy: typeof hierarchy !== 'undefined' ? hierarchy : {},
            displayTags: typeof displayTags !== 'undefined' ? displayTags : [],
            currentFamily: typeof currentFamily !== 'undefined' ? currentFamily : null,
            currentType: typeof currentType !== 'undefined' ? currentType : null,
            currentTags: typeof currentTags !== 'undefined' ? currentTags : [],
            queryParams: typeof queryParams !== 'undefined' ? queryParams : {},
            semanticSearchEnabled: typeof semanticSearchEnabled !== 'undefined' ? semanticSearchEnabled : false,
            keywordSearchEnabled: typeof keywordSearchEnabled !== 'undefined' ? keywordSearchEnabled : false
        }) %>
    </main>

    <%- include('partials/footer') %>

    <%- include('partials/chatbot-widget') %>
</body>
</html> 